<view class="container my">
  
  <view class="feature">
      <view class="me">
        <image class="avatar" mode="scaleToFill" 
          src="{{user.avatar_url}}">
        </image>
        <text class="nickname">
          {{user.nickname}}
        </text>

      </view>
      <navigator class="dream" url="dream"   hover-class="navigator-hover">
      <view class="dreams">
        
        <image class="dreams-icon" mode="scaleToFill" 
          src="../../images/cloud-blue.png">
        </image>
        <view class="dream-label">
          
           
              
               我的梦想
          
        </view>
         
      </view>
      </navigator>

  </view>
</view>
