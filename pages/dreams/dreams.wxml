<view class="container">
  
  <view class="dream-list">
    <block wx:for-items="{{dreams}}" wx:for-item="item" wx:key="dr">
      <view id="{{index}}" class="cellview">
          <text class="celltext" >{{item.dream}}</text>
          <text class="celldetail">{{item.reality}}</text>
          <text class="celldetail">{{item.progress}}</text>
      </view>
    </block>
  </view>
</view>
